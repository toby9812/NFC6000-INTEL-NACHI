INH
LETVI V18%,73
MOVEX A=1,AC=1,SM=1,M1X,L,P74,R=V30%,H=1,MS	'74
INH
LETVI V18%,74
MOVEX A=1,AC=1,SM=1,M1X,L,P75,S= 350.0,H=1,MS	'75
INH
LETVI V18%,75
*76
MOVEX A=1,AC=1,SM=1,M1X,L,P76,S= 250.0,H=1,MS	'76
INH
LETVI V18%,76
SETMD O207,1,0,5
LETVI V20%,42	'到去水取DOOR位
IF I171=1	'回原點訊號ON時
GOTO *B
ELSE
GOTO *A
ENDIF
*A
IF V2%=62
LETVI V35%,62	'紀錄PLC交握數值
GOTO *B
ELSE
GOTO *A
ENDIF
*B
SETMD O171,1,0,3
*75
MOVEX A=1,AC=1,SM=1,M1X,L,P75,R=V30%,H=1,MS	'75
INH
LETVI V18%,75
*74
MOVEX A=1,AC=1,SM=1,M1X,L,P74,R=V30%,H=1,MS	'74
INH
LETVI V18%,74
*73
MOVEX A=5,AC=1,SM=1,M1X,L,P73,R=V30%,H=1,MS	'73
INH
LETVI V18%,73
*72
MOVEX A=5,AC=1,SM=1,M1X,L,P72,R=V30%,H=1,MS	'72
INH
LETVI V18%,72
*31
MOVEX A=5,AC=1,SM=1,M1X,L,P31,R=V30%,H=1,MS	'31
INH
LETVI V18%,31
SETM O189,0	'關閉路徑訊號
*101
MOVEX A=4,AC=1,SM=1,M1X,P,P101,S=4500.0,H=1,MS	'101
INH
LETVI V18%,101
*112
MOVEX A=5,AC=1,SM=1,M1X,L,P112,R=V30%,H=1,MS	'112
INH
LETVI V18%,112
