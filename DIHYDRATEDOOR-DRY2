*113
MOVEX A=5,AC=1,SM=2,M1X,L,P113,R=V30%,H=1,MS	'113
INH
LETVI V18%,113
*114
MOVEX A=1,AC=1,SM=1,M1X,L,P114,R=V30%,H=1,MS	'114
INH
LETVI V18%,114
*115
MOVEX A=1,AC=1,SM=1,M1X,L,P115,S= 200.0,H=1,MS	'115
INH
LETVI V18%,115
SETMD O207,1,0,5
LETVI V20%,62	'到烘乾2放DOOR位置
IF I171=1	'回原點訊號ON時
SETM O211,1
DELAY 1
WAITI I211
SETM O211,0
GOTO *D
ELSE
GOTO *C
ENDIF
